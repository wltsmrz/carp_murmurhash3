(relative-include "murmurhash3.h")

(register murmur3-32x1 (Fn [&String Int] (Uint32)) "murmur3_32x1")
(register murmur3-32x4 (Fn [&String Int] (Array Uint32)) "murmur3_32x4")
(register murmur3-64x2 (Fn [&String Int] (Array Uint64)) "murmur3_64x2")

(defn-do main []
  (IO.println &(Uint32.str (murmur3-32x1 "greetings" 90210)))
  (IO.println &(Array.str &(murmur3-32x4 "greetings" 90210)))
  (IO.println &(Array.str &(murmur3-64x2 "greetings" 90210)))
)
